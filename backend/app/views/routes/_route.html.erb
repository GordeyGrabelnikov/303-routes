<tr>
  <td><%= route.name  %></td>
  <td><%= route.movement_type %></td>
  <td><%= link_to '', route, class: 'icon icon--show', title: t('helpers.links.show') %></td>
  <td><%= link_to '', edit_route_path(route), class: 'icon icon--edit', title: t('helpers.links.edit') %></td>
  <td><%= link_to '', route_path(route), method: :delete, data: {confirm: t('helpers.links.confirm')}, class: 'icon icon--delete', title: t('helpers.links.destroy') %>
</tr>

