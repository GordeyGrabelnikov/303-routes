<tr>
  <td><%= point.name  %></td>
  <td><%= point.coordinates  %></td>
  <td><%= point.tags  %></td>
  <td><%= link_to 'Show', point_path(point),  class: 'btn btn-success'  %></td>
  <td><%= link_to 'Update', edit_point_path(point),  class: 'btn btn-success'  %></td>
  <td><%= link_to 'Delete', point_path(point), method: :delete, data: {confirm: 'Are you sure?'} ,  class: 'btn btn-success' %>
</tr>